<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <patten>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} -%kvp- %msg%n</patten>
        </encoder>
    </appender>
    <root level="DEBUG">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>